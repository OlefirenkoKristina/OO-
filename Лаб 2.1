using System;

class Matrix
{
    private int[,] m;
    private int r;
    private int c;

    public Matrix(int rows, int cols)
    {
        this.r = rows;
        this.c = cols;
        m = new int[rows, cols];
    }

    public void FillRandom(int min = 0, int max = 100)
    {
        Random rand = new Random();
        for (int i = 0; i < r; i++)
        {
            for (int j = 0; j < c; j++)
            {
                m[i, j] = rand.Next(min, max);
            }
        }
    }

    public void Print()
    {
        for (int i = 0; i < r; i++)
        {
            for (int j = 0; j < c; j++)
            {
                Console.Write(m[i, j] + "\t");
            }
            Console.WriteLine();
        }
    }
}

class Program
{
    static void Main()
    {
        Matrix matrix = new Matrix(5, 5);
        matrix.FillRandom();
        matrix.Print();
    }
}
